{
	"curly" : true, // Фигурные кавычки обязательны для каждого нового блока или области.
	"eqeqeq" : true, //Контролирует использование строгого сравнения
	"immed ": true, //Обязывает оборачивать моментально вызываемые функции в скобки
	"newcap" : true, //Стимулирует использование заглавных первых букв в именах конструкторов.
	"asi" : false, // Принуждает к расстановке точек с запятой
	"camelcase" : true, // Названия переменных в camelCase стиле
	"undef" : true, // Все не глобальные переменные должны быть объявлены до их использования, Этот параметр запрещает использовать не объявленные переменные.
	"quotmark" : true, // Согласованность стиля кавычек
	"maxlen" : 120, // Максимальная длина строки
	"trailing" : true, // Запрещены пробелы на концах.
	"strict" : true, // Обязательно строгое использование.
	"browser" : true, // Стандартный браузер `window`, `document`.
	"devel" : true, // Разрешена панель разработчика `console.log;`.
	"jquery" : true, // Опция определяет глобальные переменные, предоставляемые библиотекой jQuery.
	"esversion" : 8, // Включить функции, представленные в ECMAScript 8.
	"node" : true, // Определяет доступные глобальные переменные.
	"indent": 4,// Для отступов должно быть использовано не менее 4-х пробелов
	"bitwise": true, //Этот параметр запрещает использование побитовых операторов
	"nonew": true, //Этот параметр запрещает вызывать конструктор, создавая объект, но нигде его не сохранив. 
	"regexp": true //Этот параметр запрещает использовать . в регулярных выражениях, что может быть опасным.
}

